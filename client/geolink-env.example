# XYZ-Wallet GeoLink Integration Environment Variables

# Copy this file to .env and configure your GeoLink API keys

# Backend URL
REACT_APP_BACKEND_URL=http://localhost:5000

# Mapbox Token
# Get your token from: https://account.mapbox.com/access-tokens/
REACT_APP_MAPBOX_TOKEN=your_mapbox_token_here

# GeoLink Integration
# Local development - GeoLink running on localhost
REACT_APP_GEOLINK_BASE_URL=http://localhost:3000
REACT_APP_GEOLINK_WALLET_PROVIDER_KEY=your_wallet_provider_key_here
REACT_APP_GEOLINK_DATA_CONSUMER_KEY=your_data_consumer_key_here

# Azure Production - GeoLink deployed on Azure
# REACT_APP_GEOLINK_BASE_URL=https://geolink-buavavc6gse5c9fw.westus-01.azurewebsites.net
# REACT_APP_GEOLINK_WALLET_PROVIDER_KEY=your_production_wallet_provider_key_here
# REACT_APP_GEOLINK_DATA_CONSUMER_KEY=your_production_data_consumer_key_here
